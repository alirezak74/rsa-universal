# RSA Chain Admin Panel Configuration

# JWT Secret
JWT_SECRET=your-super-secret-jwt-key-here

# Admin Credentials
ADMIN_USERNAME=admin
ADMIN_PASSWORD=admin123

# RSA Chain Network
RSA_NETWORK_URL=http://localhost:3000
RSA_HORIZON_URL=http://localhost:4000
RSA_FAUCET_URL=http://localhost:5000

# Feature Flags
ENABLE_TX_RECALL=true
ENABLE_EMERGENCY=true
ENABLE_CONTRACT_GOV=true
ENABLE_2FA=false
ENABLE_ANALYTICS=true
ENABLE_USER_MANAGEMENT=true
ENABLE_NODE_MANAGEMENT=true

# Database (for future use)
DATABASE_URL=postgresql://user:password@localhost:5432/rsa_admin

# Logging
LOG_LEVEL=info
LOG_FILE=./logs/admin.log

# Security
SESSION_SECRET=your-session-secret-here
COOKIE_SECRET=your-cookie-secret-here 